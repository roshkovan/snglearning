{{#showincourseonly}}
<div id="coursecontrols">

    {{#haseditcog}}
    <div class="editcogbutton">{{{editcog}}}</div>
    {{/haseditcog}}

    {{#hasteacherdash}} 
    <div class="teacherdashbutton"><a role="button" data-toggle="collapse" href="#teacherdashslider" aria-expanded="false" aria-controls="teacherdashslider">{{{togglebutton}}}</a></div>
    {{/hasteacherdash}}

    {{#haspermission}}{{#dashlinks}}{{#haseasyenrollment}}
    <div class="enrollcodebutton"><a href="{{{url}}}">{{{title}}}</a></div>
    {{/haseasyenrollment}}{{/dashlinks}}{{/haspermission}}

    {{#hasstudentdash}}
    <div class="thiscoursebutton"><a role="button" data-toggle="collapse" href="#studentdashslider" aria-expanded="false" aria-controls="studentdashslider">{{{togglebuttonstudent}}}</a></div>
    {{/hasstudentdash}}

</div>

<div style="clear:both;"></div>


{{#hasteacherdash}}
{{#teacherdash}}
<div class="collapse" id="teacherdashslider">
<div class="teacherdashboard">

    <div class="container-fluid">
    <div class="coursemanagementmessage">{{{coursemanagementmessage}}}</div>
    <div class="row">

        {{#hasuserpermission}}
        <div class="col-md-3 userlinks">
        <div class="dashtitle">
            <h3><i class="fa fa-users dashicon"></i>{{{userlinkstitle}}}</h3>
            <p>{{{userlinksdesc}}}</p>
        </div>
        <div class="list-group">

            {{#haspermission}}
            {{#dashlinks}}
            {{#haseasyenrollment}}
                <a href="{{{url}}}" class="list-group-item">{{{title}}}</a>
            {{/haseasyenrollment}}
            {{/dashlinks}}
            {{/haspermission}}
            {{#dashlinks}}
            {{#hasuserlinks}}
                <a href="{{{url}}}" class="list-group-item">{{{title}}}</a>
            {{/hasuserlinks}}
            {{/dashlinks}}
        </div>
        </div>
        {{/hasuserpermission}}

        <div class="col-md-3 questionlinks">
        {{#hasquestionpermission}}
        <div class="dashtitle">
            <h3><i class="fa fa-question-circle dashicon"></i>{{{qbanktitle}}}</h3>
            <p>{{{qbankdesc}}}</p>
        </div>
        <div class="list-group">
            {{#dashlinks}}
            {{#hasqbanklinks}}
                <a href="{{{url}}}" class="list-group-item">{{{title}}}</a>
            {{/hasqbanklinks}}
            {{/dashlinks}}
        </div>
        {{/hasquestionpermission}}
        
        {{#hasbadgepermission}}
        <div class="dashtitle">
            <h3><i class="fa fa-trophy dashicon"></i>{{{badgestitle}}}</h3>
            <p>{{{badgesdesc}}}</p>
        </div>
        <div class="list-group">
            {{#dashlinks}}
            {{#hasbadgelinks}}
                <a href="{{{url}}}" class="list-group-item">{{{title}}}</a>
            {{/hasbadgelinks}}
            {{/dashlinks}}
        </div>
        {{/hasbadgepermission}}
        </div>

        {{#hascoursepermission}}
        <div class="col-md-3 coursemanagelinks">
        <div class="dashtitle">
            <h3><i class="fa fa-cogs dashicon"></i>{{{coursemanagetitle}}}</h3>
            <p>{{{coursemanagedesc}}}</p>
        </div>
        <div class="list-group">
            {{#dashlinks}}
            {{#hascoursemanagelinks}}
                <a href="{{{url}}}" class="list-group-item">{{{title}}}</a>
            {{/hascoursemanagelinks}}
            {{/dashlinks}}
        </div>
        </div>
        {{/hascoursepermission}}

    </div>
    </div>

</div>
</div>
{{/teacherdash}}
{{/hasteacherdash}}


{{#hasstudentdash}}
<div class="collapse" id="studentdashslider">
<div class="studentdashboard">
<div class="container-fluid">

    <div class="row">
    
        {{#studentdashboardtextbox}}
        <div class="col-md-2">
            <div class="studentdashboardtextbox">{{{studentdashboardtextbox}}}</div>
        </div>
        {{/studentdashboardtextbox}}

        {{#hascompletionshow}}
        <div class="col-md-2 studentcompletion">
            {{#progresschart}}
                <p>{{{myprogresstext}}}</p>
                {{{progresschart}}}
            {{/progresschart}}
        </div>
        {{/hascompletionshow}}

        {{#hasgradebookshow}}
        <div class="col-md-2 studentgradebook">
            <p>{{{mygradestext}}}</p><a href="{{{gradeslinkstudent}}}" alt="{{{mygradestext}}}"><i class="fa fa-5x fa-bar-chart"></i></a>
        </div>
        {{/hasgradebookshow}}

        <div class="col-md-7 coursestafflisting">
        {{#hascoursestaffgroup}}
            <div class="dashtitle">
                <h3><i class="fa fa-{{{icon}}}">&nbsp</i>{{{title}}}</h3>
            </div>
                <div class="list-group">
                    {{#courseteachers}}
                    <div class="list-group-item">
                        <div class="staffmember">
                            {{{picture}}}
                            <div class="staffinfo">
                                <h4>{{{name}}}</h4>
                                <p><a href="mailto:{{{email}}}"><i class="fa fa-envelope-o">&nbsp</i>{{#str}}email, moodle{{/str}}</a>{{#hasmessaging}} &nbsp | &nbsp <a href="{{{messaging}}}"><i class="fa fa-comment">&nbsp</i>{{#str}}sendmessage, message{{/str}}</a>{{/hasmessaging}}</p>
                            </div>
                        </div>
                    </div>
                    {{/courseteachers}}
                </div>
                <div class="list-group">
                {{#courseother}}
                <div class="list-group-item">
                <div class="staffmember">
                    {{{picture}}}
                    <div class="staffinfo">
                        <h4>{{{name}}}</h4>
                        <p><a href="mailto:{{{email}}}"><i class="fa fa-envelope-o">&nbsp</i>{{#str}}email, moodle{{/str}}</a>{{#hasmessaging}} &nbsp | &nbsp <a href="{{{messaging}}}"><i class="fa fa-comment">&nbsp</i>{{#str}}sendmessage, message{{/str}}</a>{{/hasmessaging}}</p>
                    </div>
                </div>
                </div>
            {{/courseother}}
            </div>
        {{/hascoursestaffgroup}}
        </div>
    </div>
</div>
</div>
</div>
{{/hasstudentdash}}

<div style="clear:both;"></div>

{{/showincourseonly}}